<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Parametrized Tests">

    <test name="1st Test">
        <parameter name="usr" value="duotech2023"/>
        <parameter name="pass" value="duotech"/>

        <classes>
            <class name="demo.ParametrizationSimple"/>
        </classes>

    </test>


    <test name="2nd Test">
        <parameter name="usr" value="duotech2050"/>
        <parameter name="pass" value="duotech123"/>

        <classes>
            <class name="demo.ParametrizationSimple"/>
        </classes>

    </test>

    <test name="3rd Test">
        <parameter name="usr" value="coolherc2055"/>
        <parameter name="pass" value="coolherc"/>

        <classes>
            <class name="demo.ParametrizationSimple"/>
        </classes>

    </test>


</suite>